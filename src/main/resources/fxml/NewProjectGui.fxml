<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="237.0" prefWidth="616.0" spacing="10.0" xmlns="http://javafx.com/javafx/8.0.191" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.skide.gui.controllers.CreateProjectGUIController">
   <children>
      <Label text="Name" />
      <TextField fx:id="projectNameField" prefHeight="25.0" prefWidth="328.0" promptText="Project Name" />
      <Label text="Project path" />
      <HBox prefHeight="38.0" prefWidth="600.0" spacing="5.0">
         <children>
            <TextField fx:id="projectPathField" prefHeight="25.0" prefWidth="536.0" promptText="Project Path" />
            <Button fx:id="choosePathButton" mnemonicParsing="false" text="..." />
         </children>
      </HBox>
      <Label text="Skript version" />
      <ComboBox fx:id="skriptVersionComboBox" prefWidth="150.0" />
      <HBox alignment="CENTER_RIGHT" prefHeight="30.0" prefWidth="600.0" spacing="5.0">
         <children>
            <CheckBox fx:id="openAfterCreation" mnemonicParsing="false" text="Open after creation" />
            <Button fx:id="cancelButton" mnemonicParsing="false" text="Cancel" />
            <Button fx:id="createButton" mnemonicParsing="false" text="Create" />
         </children>
      </HBox>
   </children>
   <padding>
      <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
   </padding>
</VBox>
