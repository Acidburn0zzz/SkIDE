<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<VBox xmlns:fx="http://javafx.com/fxml/1" prefHeight="200.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.191"
      fx:controller="com.skide.gui.controllers.ImportProjectGUIController">
    <children>
        <Label text="Name"/>
        <TextField fx:id="projectNameField" prefHeight="25.0" prefWidth="328.0" promptText="Project Name"/>
        <Label text="Folder Path"/>
        <HBox prefHeight="61.0" prefWidth="591.0" spacing="5.0">
            <children>
                <TextField fx:id="projectPathField" prefHeight="25.0" prefWidth="536.0" promptText="The Folder path"/>
                <Button fx:id="choosePathButton" mnemonicParsing="false" text="..."/>
            </children>
        </HBox>
        <Label text="Skript Version"/>
        <ComboBox fx:id="skriptVersionComboBox" prefWidth="150.0"/>
        <HBox alignment="CENTER_RIGHT" prefHeight="32.0" prefWidth="600.0" spacing="10.0">
            <children>
                <CheckBox fx:id="openAfterCreation" mnemonicParsing="false" prefHeight="30.0" prefWidth="126.0"
                          text="Open after creation"/>
                <Button fx:id="cancelButton" mnemonicParsing="false" text="Cancel"/>
                <Button fx:id="createButton" mnemonicParsing="false" text="Create"/>
            </children>
        </HBox>
    </children>
    <padding>
        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
    </padding>
</VBox>
